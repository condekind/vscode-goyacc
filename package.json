{
    "name": "goyacc",
    "displayName": "goyacc",
    "description": "Yacc Syntax Highlighting with Golang instead of C (goyacc)",
    "version": "0.0.1",
    "publisher": "condekind",
    "repository": {
        "url": "https://github.com/condekind/vscode-goyacc"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bison",
            "aliases": ["Bison", "bison"],
            "extensions": [".y", ".go.y"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bison",
            "scopeName": "source.bison",
            "path": "./syntaxes/bison.tmLanguage"
        }]
    }
}